{
	"name": "Kreide",
	"semanticHighlighting": false,
	"colors": {
		"activityBar.background": "#1d1d1d",
		"activityBar.foreground": "#f0f0f0",
		"activityBar.inactiveForeground": "#707070",
		"activityBarBadge.background": "#0486e2",
		"breadcrumb.background": "#262626",
		"editor.background": "#222222",
		"editor.foreground": "#f0f0f0",
		"editor.lineHighlightBackground": "#404040",
		"editor.selectionBackground": "#404040",
		"editorCursor.foreground": "#f0f0f0",
		"editorGroupHeader.tabsBackground": "#1d1d1d",
		"editorGutter.background": "#1d1d1d",
		"editorWhitespace.foreground": "#404040",
		"input.background": "#1d1d1d",
		"menu.background": "#222222",
		"sideBar.background": "#262626",
		"sideBar.foreground": "#f0f0f0",
		"sideBarSectionHeader.background": "#404040",
		"tab.activeBackground": "#262626",
		"tab.inactiveBackground": "#1d1d1d",
		"terminal.background": "#222222",
		"editorBracketHighlight.foreground1": "#bbb",
		"editorBracketHighlight.foreground2": "#999",
		"editorBracketHighlight.foreground3": "#ddd",
		"editorBracketHighlight.foreground4": "#bbb",
		"editorBracketHighlight.foreground5": "#999",
		"editorBracketHighlight.foreground6": "#ddd",
		"editorBracketHighlight.unexpectedBracket.foreground": "#cc3333"
	},
	"tokenColors": [
		{
			"name": "Background",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#808080"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"keyword",
				"keyword.operator.key-value",
				"keyword.operator.namespace",
				"keyword.operator.arrow.skinny",
				"meta.brace"
			],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Main content",
			"scope": [
				"variable",
				"variable.parameter",
				"support.type.property-name.json",
				"meta.property-name",
				"meta.object-literal.key",
				"meta.method.body.java"
			],
			"settings": {
				"foreground": "#f0f0f0"
			}
		},
		{
			"name": "Supporting content",
			"scope": [
				"storage.modifier",
				"entity.other",
				"keyword.other.rust",
				"keyword.other",
				"keyword.control.import",
				"keyword.control.from",
				"keyword.control.default",
				"keyword.control.export",
				"variable.language.super",
				"entity.name.namespace",
				"entity.name.module",
				"keyword.control.directive.include",
				"punctuation.definition.directive",
				"keyword.declaration.dart"
			],
			"settings": {
				"foreground": "#bb99ee"
			}
		},
		{
			"name": "Actions",
			"scope": [
				"keyword.operator",
				"entity.name.function",
				"support.function",
				"keyword.operator.dereference",
				"keyword.operator.borrow"
			],
			"settings": {
				"foreground": "#88ccff"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"string",
				"constant",
				"support.type.property-attribute",
				"meta.property-value",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#ffbb77"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"variable.other.object.property",
				"variable.other.property",
				"support.variable.property",
				"source.rust",
				"meta.function.call.rust",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#99ddaa"
			}
		},
		{
			"name": "Control flow",
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.logical",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.ternary",
				"keyword.operator.arrow.fat",
				"keyword.operator.question",
				"punctuation.accessor.optional"
			],
			"settings": {
				"foreground": "#eeaadd"
			}
		},
		{
			"name": "Descriptors",
			"scope": [
				"entity.name",
				"storage.type",
				"keyword.other.fn",
				"entity.name.type",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#eedd66"
			}
		},
		{
			"name": "Meta",
			"scope": [
				"meta.use",
				"meta.attribute",
				"entity.name.type.lifetime",
				"entity.name.function.preprocessor",
				"storage.type.annotation",
				"punctuation.definition.annotation"
			],
			"settings": {
				"foreground": "#cc9988"
			}
		},
		{
			"name": "Important",
			"scope": [
				"variable.language.this",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#ff8888"
			}
		},
		{
			"name": "Error",
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#cc3333"
			}
		}
	]
}